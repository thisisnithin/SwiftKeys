{
  "name": "swift-keys",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "npm": ">=8.0.0",
    "node": ">=16.0.0"
  },
  "packageManager": "yarn@1.22.10",
  "scripts": {
    "startAll": "concurrently \"npm run nextDev\" \"npm run wundergraph\" \"npm run browser\"",
    "dev": "next dev",
    "start": "next start",
    "build": "next build",
    "wundergraph": "wunderctl up --debug",
    "generate": "wunderctl generate --debug",
    "wunderctl": "wunderctl",
    "storage": "cd minio && sh setup.sh && docker-compose logs -f -t",
    "database": "cd database && docker-compose up",
    "migrate": "prisma format && prisma migrate dev --name",
    "seed": "ts-node ./database/seed.ts && npm run open",
    "browser": "wait-on \"http-get://localhost:3000\" && wait-on \"http-get://localhost:9991\" && open-cli http://localhost:3000",
    "prepare": "husky install",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "next lint --fix"
  },
  "dependencies": {
    "@emotion/css": "^11.10.0",
    "@headlessui/react": "^1.6.6",
    "@wundergraph/nextjs": "^0.1.2",
    "@wundergraph/sdk": "^0.98.1",
    "graphql": "^16.3.0",
    "next": "^12.1.6",
    "random-words": "^1.2.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-icons": "^4.4.0"
  },
  "devDependencies": {
    "@svgr/webpack": "^6.3.1",
    "@types/node": "^17.0.27",
    "@types/react": "^18.0.7",
    "autoprefixer": "^10.4.8",
    "concurrently": "^6.0.0",
    "eslint": "8.23.0",
    "eslint-config-next": "12.2.5",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "open-cli": "^7.0.1",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "prisma": "^4.3.1",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.8.2",
    "wait-on": "^6.0.0"
  },
  "prisma": {
    "schema": "database/schema.prisma"
  }
}
